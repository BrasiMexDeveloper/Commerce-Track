@model MyViewModel

<div class="">
    <h1>Orders</h1>
    <table class="table table-striped">
        <tr>
            <th>Customer Name</th>
            <th>Product</th>
            <th>Quantity</th>
            <th>Orders on</th>
        </tr>
        @foreach(Order o in Model.AllOrders)
        {
            <tr>
                <td>@o.Customer.Name</td>
                <td>@o.Product.Name</td>
                <td>@o.QuantityBought</td>
                <td>@o.CreatedAt.ToString("MMMM dd, yyyy")</td>
            </tr>
        }
    </table>
    <div>
        @await Html.PartialAsync("_OrderForm", Model.Order)
    </div>
</div>